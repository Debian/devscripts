#, no-wrap
msgid "B<--help>, B<--version>"
msgstr "B<--help>, B<--version>"

#. type: Plain text
#: ../scripts/cvs-debc.1:58 ../scripts/cvs-debi.1:62
#: ../scripts/cvs-debrelease.1:64 ../scripts/debc.1:92 ../scripts/debi.1:109
