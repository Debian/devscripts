msgstr ""

#. type: Plain text
#: ../scripts/debuild.1:220 ../scripts/debuild.1:228 ../scripts/debuild.1:235
msgid "Hook is run from parent directory of unpacked source."
msgstr ""

#. type: Plain text
#: ../scripts/debuild.1:222
msgid "Corresponds to B<dpkg>'s B<check> hook."
msgstr ""

#. type: TP
#: ../scripts/debuild.1:222
#, no-wrap
