msgstr ""

#. type: Plain text
#: ../scripts/debuild.1:189
msgid "Corresponds to B<dpkg>'s B<build> hook."
msgstr ""

#. type: TP
#: ../scripts/debuild.1:189
#, no-wrap
msgid "B<dpkg-binary-hook>"
msgstr ""

#. type: Plain text
#: ../scripts/debuild.1:193
msgid ""
"Run between B<debian/rules build> and B<debian/rules binary>(B<-arch>).  Run "
